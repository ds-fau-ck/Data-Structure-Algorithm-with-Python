    "def bubleSort(arr):\n",
    "    for i in range(len(arr)):\n",
    "        for j in range(len(arr)-i-1):\n",
    "            if arr[j]>arr[j+1]:\n",
    "                arr[j],arr[j+1]=arr[]\n",
    "arr=[5,1,2,4,7,6]\n",
    "bubleSort(arr)\n",
    "print(arr)"
   ]
